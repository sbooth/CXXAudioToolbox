BasedOnStyle: LLVM

ColumnLimit: 120

IndentWidth: 4
TabWidth: 4
UseTab: Never

ContinuationIndentWidth: 8
ConstructorInitializerIndentWidth: 4

InsertBraces: true

IncludeBlocks: Regroup
IncludeCategories:
  - Regex: '^".*"$'
    Priority: 1
  - Regex: '^<audio_toolbox/'
    Priority: 1
  - Regex: '^<core_audio/'
    Priority: 2
  - Regex: '^<(AudioToolbox|AVFAudio)/'
    Priority: 3
  - Regex: '^<[^./>]+>$'
    Priority: 10
  - Regex: '.*'
    Priority: 20
